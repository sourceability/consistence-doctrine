{
	"name": "consistence/consistence-doctrine",
	"description": "Integration of Consistence library with Doctrine ORM",
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.4 || ~8.0",
		"consistence-community/consistence": "~2.1",
		"doctrine/annotations": "~1.7",
		"doctrine/orm": "^2.6.3"
	},
	"require-dev": {
		"consistence-community/coding-standard": "3.11.0",
		"doctrine/persistence": "^1.3.5 || ^2.0",
		"php-parallel-lint/php-parallel-lint": "1.2.0",
		"phing/phing": "2.16.4",
		"phpunit/phpunit": "9.5.2"
	},
	"autoload": {
		"psr-4": { "Consistence\\Doctrine\\": [ "src" ] },
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": { "Consistence\\Doctrine\\": [ "tests" ] },
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
